<div class="tx-winetreatment-category">
	<table>
		<f:for each="{result}" as="part">
			<tr>
				<f:for each="{part}" as="category">
					<f:if condition="{category.products}">
						<f:then>
							<td>
								<f:link.action action="show" arguments="{category : category}" title="{category.productCount} <f:translate key='category_show_products' />">{category.name}</f:link.action>
							</td>
						</f:then>
					</f:if>
				</f:for>
			</tr>
		</f:for>
	</table>
</div>
